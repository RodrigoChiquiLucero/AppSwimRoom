<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appswim">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="SwimmerApp"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateSwimmerActivity"
            android:label="CreateSwimmerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".UpdateSwimmerActivity"
            android:label="UpdateSwimmerActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".SwimmerActivity"
            android:label="SwimmerActivity"
            android:screenOrientation="portrait">
        </activity>

    </application>

</manifest>